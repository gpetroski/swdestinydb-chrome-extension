  {
    "name": "SWDestinyDB Search Buttons",
    "version": "1.0",
    "description": "Add search buttons from deck building interface to the main /find search page of swdestinydb.com",
    "permissions": ["activeTab", "declarativeContent"],
    "homepage_url": "https://github.com/gpetroski/swdestinydb-chrome-extension",
    "content_scripts" : [{
      "run_at": "document_idle",
      "matches": ["https://swdestinydb.com/find*"],
      "js": ["content.js"]
    }],
    "manifest_version": 2
  }